IDR_MAIN_MENU MENU 
BEGIN
    POPUP "Layouts"
    BEGIN
        MENUITEM "New\tCtrl+N",                 ID_LAYOUTS_NEW
        MENUITEM "Edit\tCtrl+Enter",            ID_LAYOUTS_EDIT
        MENUITEM SEPARATOR
        MENUITEM "Load\tCtrl+L, Ctrl+O",        ID_LAYOUTS_LOAD
        MENUITEM "Save\tCtrl+S",                ID_LAYOUTS_SAVE
        MENUITEM "Save As\tCtrl+Shift+S",       ID_LAYOUTS_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "Import",                      ID_LAYOUTS_IMPORT
        MENUITEM "Export",                      ID_LAYOUTS_EXPORT
        MENUITEM SEPARATOR
        MENUITEM "Undo\tCtrl+Z",                ID_LAYOUTS_UNDO
        MENUITEM "Redo\tCtrl+Y, Ctrl+Shift+Z",  ID_LAYOUTS_REDO
    END
    POPUP "Data sources"
    BEGIN
        MENUITEM "Edit\tCtrl+D",                ID_DATASOURCES_EDIT
    END
    POPUP "Tables"
    BEGIN
        MENUITEM "Edit\tCtrl+T",                ID_TABLES_EDIT
    END
    POPUP "Layers"
    BEGIN
        MENUITEM "Add\tInsert",                 ID_LAYERS_ADD
        MENUITEM "Remove\tDelete",              ID_LAYERS_REMOVE
        MENUITEM "Edit\tEnter",                 ID_LAYERS_EDIT
        MENUITEM SEPARATOR
        MENUITEM "Edit list\tCtrl+Shift+L",     ID_LAYERS_EDITLIST
        MENUITEM SEPARATOR
        MENUITEM "Move to top\tHome",           ID_LAYERS_MOVETOTOP
        MENUITEM "Move up\tPage Up",            ID_LAYERS_MOVEUP
        MENUITEM "Move down\tPage Down",        ID_LAYERS_MOVEDOWN
        MENUITEM "Move to bottom\tEnd",         ID_LAYERS_MOVETOBOTTOM
        MENUITEM SEPARATOR
        MENUITEM "Copy\tCtrl+C",                ID_LAYERS_COPY
        MENUITEM "Paste\tCtrl+V",               ID_LAYERS_PASTE
    END
    POPUP "View"
    BEGIN
        MENUITEM "Reset view zoom",             ID_VIEW_RESETVIEWZOOM
        MENUITEM SEPARATOR
        MENUITEM "Snap to grid",                ID_VIEW_SNAPTOGRID
        MENUITEM SEPARATOR
        MENUITEM "Show grid",                   ID_VIEW_SHOWGRID
        MENUITEM "Show checkerboard",           ID_VIEW_SHOWCHECKERBOARD
