    POPUP "Tools"
    BEGIN
        MENUITEM "Show hypertext\tCtrl+Shift+H", ID_TOOLS_SHOWHYPERTEXT
        MENUITEM "Optimize hypertext\tCtrl+Shift+O", ID_TOOLS_OPTIMIZEHYPERTEXT
    END
END

